{
  "name": "sudt-cli",
  "version": "0.5.0",
  "description": "This is a command line tool to create SUDT tokens on the Nervos Mainnet and Testnet.",
  "main": "src/index.ts",
  "scripts": {
    "start": "npx nodemon -w src -e ts,js,json --exec \"npx ts-node --files src/index.ts\"",
    "start-help": "npx nodemon -w src -e ts,js,json --exec \"npx ts-node --files src/index.ts --help\"",
    "build": "npx tsc",
    "bump": "npm --no-git-tag-version version patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordanmack/sudt-cli.git"
  },
  "author": "Jordan Mack <jordan@nervos.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jordanmack/sudt-cli/issues"
  },
  "homepage": "https://github.com/jordanmack/sudt-cli#readme",
  "dependencies": {
    "@lay2/pw-core": "^0.4.0-alpha.8",
    "@nervosnetwork/ckb-sdk-utils": "^0.41.1",
    "blake2b": "^2.1.3",
    "ckb-js-toolkit": "^0.10.2",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.4",
    "@types/node-fetch": "^2.5.10",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
